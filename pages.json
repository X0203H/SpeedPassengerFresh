{
	"pages": [ //pages数组中第一项表示应用启动页，参考：https://uniapp.dcloud.io/collocation/pages
		{
			"path": "pages/home/home",
			"style": {
				"navigationBarTitleText": "速客优鲜"
			}
		}, {
			"path": "pages/cart/cart",
			"style": {
				"navigationBarTitleText": "购物车",
				"enablePullDownRefresh": false
			}
		}, {
			"path": "pages/classify/classify",
			"style": {
				"navigationBarTitleText": "分类",
				"enablePullDownRefresh": false
			}
		}, {
			"path": "pages/mine/mine",
			"style": {
				"navigationBarTitleText": "我的",
				"enablePullDownRefresh": false
			}
		}
	],
	"subPackages": [{
		"root": "subpkg", // 分包根路径
		"pages": [{
			"path": "login/login",
			"style": {
				"navigationBarTitleText": "登录",
				"enablePullDownRefresh": false
			}

		}, {
			"path": "register/register",
			"style": {
				"navigationBarTitleText": "注册",
				"enablePullDownRefresh": false
			}

		}] // 分包的页面
	}],
	"globalStyle": {
		"navigationBarTextStyle": "black",
		"navigationBarTitleText": "uni-app",
		"navigationBarBackgroundColor": "#F8F8F8",
		"backgroundColor": "#F8F8F8",
		"usingComponents": {
			"van-button": "/wxcomponents/vant/dist/button/index",
			"van-image": "/wxcomponents/vant/dist/image/index",
			"van-count-down": "/wxcomponents/vant/dist/count-down/index"
		}
	},
	"tabBar": {
		"list": [{
				"pagePath": "pages/home/home",
				"text": "首页",
				"iconPath": "static/images/tabbar/shouye.png",
				"selectedIconPath": "static/images/tabbar/shouye-active.png"
			},
			{
				"pagePath": "pages/cart/cart",
				"text": "购物车",
				"iconPath": "static/images/tabbar/gouwuche.png",
				"selectedIconPath": "static/images/tabbar/gouwuche-active.png"
			},
			{
				"pagePath": "pages/classify/classify",
				"text": "分类",
				"iconPath": "static/images/tabbar/fenlei.png",
				"selectedIconPath": "static/images/tabbar/fenlei-active.png"
			},
			{
				"pagePath": "pages/mine/mine",
				"text": "我的",
				"iconPath": "static/images/tabbar/wode.png",
				"selectedIconPath": "static/images/tabbar/wode-active.png"
			}
		]
	},
	"uniIdRouter": {},
	"condition": { //模式配置，仅开发期间生效
		"current": 1, //当前激活的模式(list 的索引项)
		"list": [{
			"name": "", //模式名称
			"path": "", //启动页面，必选
			"query": "" //启动参数，在页面的onLoad函数里面得到
		}]
	}
}